{
  "name": "nest-responses-generator-plugin",
  "version": "0.0.1",
  "description": "NestJS plugin that automatically generates Swagger response JSON examples from service's return types",
  "main": "plugin.js",
  "repository": {
    "url": "https://github.com/sand97/nest-responses-generator"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "plugin.js"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "nestjs",
    "swagger",
    "openapi",
    "typescript",
    "response-types",
    "automatic-generation"
  ],
  "author": "Bruce Guenkam",
  "license": "MIT",
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/swagger": "^7.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "glob": "^10.3.0",
    "typescript": "^5.3.0"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0"
  }
}
